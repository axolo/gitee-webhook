{
  "name": "gitee-webhook",
  "version": "0.0.4",
  "description": "Gitee WebHook.",
  "private": true,
  "scripts": {
    "start": "egg-scripts start --daemon --port 7999 --title=egg-server-webhook",
    "stop": "egg-scripts stop --title=egg-server-webhook",
    "dev": "egg-bin dev --port 7999",
    "debug": "egg-bin debug",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov",
    "autod": "autod"
  },
  "egg": {
    "declarations": true
  },
  "dependencies": {
    "egg": "^2.29.3",
    "egg-scripts": "^2.13.0"
  },
  "devDependencies": {
    "autod": "^3.1.1",
    "autod-egg": "^1.1.0",
    "egg-bin": "^4.15.0",
    "egg-ci": "^1.18.0",
    "egg-mock": "^4.1.0",
    "eslint": "^7.23.0",
    "eslint-config-egg": "^9.0.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "ci": {
    "version": "10"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Yueming Fang",
  "license": "MIT"
}
